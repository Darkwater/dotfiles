[global_config]
  custom_url_handler = xdg-open
  enabled_plugins = LaunchpadCodeURLHandler, APTURLHandler, LaunchpadBugURLHandler
  use_custom_url_handler = True
  geometry_hinting = False
  title_transmit_bg_color = "#000000"
  inactive_color_offset = 1.0
  title_inactive_bg_color = "#000000"
[keybindings]
  cycle_prev = None
  zoom_in = None
  go_up = None
  zoom_out = None
  close_window = None
  full_screen = <Primary>F11
  zoom_normal = None
  go_down = None
  close_term = None
  cycle_next = None
  go_right = None
  go_left = None
  hide_window = <Shift><Control>a
[profiles]
  [[default]]
    foreground_color = "#c5c8c6"
    palette = "#000204:#cd0000:#00cd00:#b8b800:#1e90ff:#cd00cd:#00cdcd:#e5e5e5:#4c4c4c:#ff0000:#00ff00:#ffff00:#4682b4:#ff00ff:#00ffff:#ffffff"
    background_image = None
    background_darkness = 0.86
    urgent_bell = True
    background_type = transparent
    use_system_font = False
    cursor_color = "#848484"
    scroll_background = False
    scroll_on_output = False
    icon_bell = False
    scrollbar_position = hidden
    show_titlebar = False
    color_scheme = custom
    font = Droid Sans Mono 8
    background_color = "#000204"
    scrollback_lines = 5000
    cursor_blink = False
[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = default
    [[[window0]]]
      type = Window
      parent = ""
[plugins]
